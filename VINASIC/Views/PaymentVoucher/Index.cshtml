<div class="row">
    <div class="col-lg-12">
        @*<h3 style="background-color: #faf9f8" class="page-header">
            <img style="width: 200px" src="~/Files/logo.jpg" />
        </h3>*@
        <ol class="breadcrumb">
            <li><i class="fa fa-home"></i><a href="/DashBoard/Index">Home</a></li>
            <li><i class="fa fa-laptop"></i>Phiếu Chi</li>
        </ol>
    </div>
</div>
<div style="margin-bottom: 15px;" class="row">
    
</div>
<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading tab-bg-info">
                <ul class="nav nav-tabs">
                    <li id="ProcessOrder" class="active">
                        <a data-toggle="tab" href="#recent-activity">
                            <i class="icon-home"></i>
                            Phiếu Chi
                        </a>
                    </li>
                    <li id="CreateOrder" class="">
                        <a data-toggle="tab" href="#edit-profile">
                            <i class="icon-envelope"></i>
                            Tạo Mới/Chỉnh Phiếu Chi
                        </a>
                    </li>
                </ul>
            </header>
            <div style="border-style: none" class="panel-body pannel-order">
                <div class="tab-content">
                    <div id="recent-activity" class="tab-pane active">
                        <div style="margin-bottom: 15px;" class="row">
                            <div class="form-group">
                                <div class="col-md-3 col-sm-4 col-xs-12">
                                    <label for="datefrom">Từ Ngày</label>
                                    <input type="date" class="form-control" name="datefrom" value="" id="datefrom" />
                                </div>
                                <div class="col-md-3 col-sm-4 col-xs-12">
                                    <label for="dateto">Đến Ngày</label>
                                    <input type="date" class="form-control" name="dateto" value="" id="dateto" />
                                </div>
                                <div class="col-md-3 col-sm-4 col-xs-12">
                                    <label for="keyword">Từ Khóa</label>
                                    <input type="text" class="form-control" id="keyword" placeholder="">
                                </div>
                                <div style="margin-top:25px" class="col-md-3 col-sm-4 col-xs-12">
                                    <button id="search" class="btn button btn-primary">Tìm Kiếm</button>
                                </div>

                            </div>
                        </div>
                        
                        <div class="=rows" id="jtablePaymentVoucher"></div>
                    </div>
                <div id="edit-profile" class="tab-pane">
                    <section class="panel">
                        <div class="panel-body bio-graph-info">
                            <div class="col-lg-5">
                                <section class="panel">
                                    <header class="panel-heading">
                                        Thông Tin Phiếu Chi
                                    </header>
                                    <div class="panel-body">
                                        <form role="form">
                                            <div class="form-group">
                                                <label for="cname">Loại Chi</label>
                                                <select id="cemployee" class="form-control input-lg m-bot15">
                                                    <option value="0">Thông Thường </option>
                                                    <option value="1">Nhập Hàng</option>
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <label for="cname">Người Nhận</label>
                                                <input type="text" class="form-control" id="cname" placeholder="">
                                            </div>
                                            <div class="form-group">
                                                <label for="cphone">Số Điện Thoại</label>
                                                <input type="text" class="form-control" id="cphone" placeholder="" onkeypress=" return isNumberKey(event)">
                                            </div>
                                            <div class="form-group">
                                                <label for="caddress">Địa Chỉ</label>
                                                <input type="text" class="form-control" id="caddress" placeholder="">
                                            </div>
                                            <div class="form-group">
                                                <label for="content">Nội Dung</label>
                                                <input type="text" class="form-control" id="content" placeholder="">
                                            </div>
                                            <div class="form-group">
                                                <label id="dsubtotal2"  for="cmail">Tổng Tiền(VND)</label>
                                                <input id="dsubtotal1" type="text" class="form-control detail" placeholder="" onkeypress=" return isNumberKey(event) ">
                                            </div>
                                        </form>
                                    </div>
                                </section>
                            </div>
                            <div id="viewDetail" class="col-lg-7">
                                <section class="panel">
                                    <header class="panel-heading">
                                        Thông Tin Chi Tiết
                                    </header>
                                    <div class="panel-body">
                                        <div class="col-lg-6">
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="dproduct">Dịch Vụ</label>
                                                <input style="min-width:290px" type="text" id="dproduct" class="form-control input-lg m-bot15 detail">
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="rows">
                                                <div style="display:@ViewBag.CssShowDim" class="col-md-2 forPrint">
                                                    <label for="dwidth">CNgang(m)</label>
                                                    <input id="dwidth" type="text" pattern="[0-9]*" class="form-control detail caculator" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div style="display:@ViewBag.CssShowDim" class="col-md-2 forPrint">
                                                    <label for="dheignt">CCao(m)</label>
                                                    <input id="dheignt" type="text" class="form-control detail caculator" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div style="display:none" class="col-md-2 forPrint">
                                                    <label for="dsquare">DTích(m2)</label>
                                                    <input id="dsquare" type="text" class="form-control detail" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div class="col-md-2">
                                                    <label for="dquantity">SLượng</label>
                                                    <input id="dquantity" type="text" class="form-control detail caculator caculatorTotal" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div style="display:@ViewBag.CssShowDim" class="col-md-2 forPrint">
                                                    <label for="dsumsquare">TổngDT</label>
                                                    <input id="dsumsquare" type="text" class="form-control detail" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div class="col-md-2">
                                                    <label for="dprice">Đơn Giá</label>
                                                    <input style="min-width:90px" id="dprice" type="text" class="form-control detail caculator caculatorTotal" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>
                                                <div class="col-md-2">
                                                    <label for="dsubtotal">T Tiền</label>
                                                    <input style="width:110px" id="dsubtotal" type="text" class="form-control detail" placeholder="" onkeypress=" return isNumberKey(event) ">
                                                </div>

                                            </div>
                                        </div>
                                        @*<div class="col-lg-6 forPrint">
                                            <label for="dfilename">Tên File</label>
                                            <input id="dfilename" type="text" class="form-control detail" placeholder="">
                                        </div>*@
                                        <div class="col-lg-12">
                                            <label for="dnote">Ghi Chú</label>
                                            <input style="min-width:290px" id="dnote" type="text" class="form-control detail" placeholder="">
                                        </div>
                                        <a id="resetDetail" style="margin-top: 20px; margin-bottom: 10px; margin-left: 10px" title="" class="btn btn-warning btn-sm" href="javascript:void(0)">Reset</a>
                                        <br />
                                        <div id="jtableOrderDetail"></div>
                                        <div class="row">
                                            <input style="text-align: right; margin-top: 10px; margin-right: 12px; width: 200px; float: right" id="dtotal" type="text" class="form-control dsubtotal" placeholder="">
                                            <strong style="margin-top: 10px; width: 200px; float: right">Tổng Tiền (VND)</strong>
                                        </div>
                                        @*<a id="saveOrder" style="margin-left: 20px; margin-top: 20px" title="" class="btn btn-primary btn-sm" href="javascript:void(0)">Lưu Đơn Hàng</a>*@
                                        <a id="resetOrder" style="margin-top: 20px" title="" class="btn btn-warning btn-sm" href="javascript:void(0)">Reset</a>
                                        <a id="canelOrder" style="margin-top: 20px" title="" class="btn btn-danger btn-sm" href="javascript:void(0)">Hủy Bỏ</a>
                                    </div>
                                </section>
                            </div>
                            <a id="saveOrder" style="margin-left: 20px; margin-top: 20px" title="" class="btn btn-primary btn-sm" href="javascript:void(0)">Lưu Phiếu Chi</a>
                        </div>
                       
                    </section>

                    </div>
                </div>
            </div>
        </section>
    </div>
</div>

